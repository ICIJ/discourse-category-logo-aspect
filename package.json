{
  "name": "discourse-category-logo-aspect",
  "version": "0.0.1",
  "repository": "https://github.com/ICIJ/discourse-category-logo-aspect",
  "author": "ICIJ",
  "license": "MIT",
  "scripts": {
    "lint": "yarn eslint --ext .js,.js.es6 --no-error-on-unmatched-pattern test javascripts",
    "lint:fix": "yarn eslint --fix --ext .js,.js.es6 --no-error-on-unmatched-pattern test javascripts",
    "ember": "yarn ember-template-lint --no-error-on-unmatched-pattern javascripts",
    "ember:fix": "yarn ember-template-lint --fix --no-error-on-unmatched-pattern javascripts",
    "prettify": "yarn prettier --list-different '{javascripts,common,mobile}/**/*.{scss,js,es6,hbs}'",
    "prettify:fix": "yarn prettier --write '{javascripts,common,mobile}/**/*.{scss,js,es6,hbs}'"
  },
  "devDependencies": {
    "@discourse/lint-configs": "^1.0.0",
    "ember-template-lint": "^5.11.2",
    "eslint": "^8.52.0",
    "prettier": "^2.8.8"
  }
}
